{
  "targets": [
    {
      "paths": [
        "src/entities/**/ui/**",
        "src/features/**/ui/**",
        "src/widgets/**/ui/**",
        "src/shared/**/ui/**"
      ],
      "fileExtensions": [
        ".tsx",
        ".ts"
      ],
      "outputFile": "index.ts",
      "exportStyle": "auto"
    },
    {
      "paths": [
        "public/assets/icons"
      ],
      "fileExtensions": [
        ".svg"
      ],
      "outputFile": "index.ts",
      "exportStyle": "named",
      "namingConvention": "PascalCase",
      "fromWithExtension": true
    }
  ]
}
